{
  "manifest_version": 2,
  "name": "Dev Checklist",
  "description": "",
  "version": "1.0",
  "browser_action": {
    "default_icon":
    {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html"
  },
  "icons":
  {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "https://docs.google.com/spreadsheets/*",
    "https://script.google.com/*"
  ]
}